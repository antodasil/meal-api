  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch MealApp",
      "type": "pwa-node",
      "request": "launch",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
      "args": ["./src/index.ts", "--transpile-only"],
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      "outputCapture": "std"
    }
  ]
}
